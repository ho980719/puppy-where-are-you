{
  "name": "@puppy/api",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prestart:dev": "pnpm -w --filter @puppy/shared build",
    "start:dev": "node -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "prestart:prod": "pnpm -w --filter @puppy/shared build && pnpm --filter @puppy/api build",
    "start:prod": "node dist/main.js",
    "dev": "pnpm run start:dev",
    "prebuild": "pnpm -w --filter @puppy/shared build",
    "build": "nest build",
    "lint": "eslint .",
    "format": "prettier -w .",
    "prisma": "prisma",
    "postinstall": "prisma generate",
    "seed": "ts-node ./prisma/seed.ts",
    "seed:places": "ts-node ./prisma/seed-places.ts",
    "seed:reviews": "ts-node ./prisma/seed-reviews.ts"
  },
  "dependencies": {
    "@puppy/shared": "workspace:*",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "7.4.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "helmet": "7.1.0",
    "reflect-metadata": "0.1.13",
    "nestjs-zod": "3.0.0",
    "zod": "3.23.8",
    "rxjs": "^7.8.1",
    "@prisma/client": "5.15.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.0.0",
    "@types/express": "4.17.21",
    "@types/node": "20.11.30",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "prisma": "5.15.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  }
}
